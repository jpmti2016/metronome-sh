{
  "compilerOptions": {
    "module": "esnext",
    "lib": ["ES2019", "DOM"],
    "target": "ES2019",
    "rootDir": "./src",
    "strict": true,
    "types": ["node"],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop":true,
    "moduleResolution": "node",
    "outDir": "./dist"
  },

  "exclude": ["./example", "./node_modules", "./dist"]
}

